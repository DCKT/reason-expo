{
  "name": "reason-expo",
  "bsc-flags": ["-bs-super-errors"],
  "reason": { "react-jsx": 3 },
  "refmt": 3,
  "bs-dependencies": [
    "reason-react",
    "reason-react-native-monorepo/reason-react-native"
  ],
  "namespace": true,
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "suffix": ".bs.js",
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "warnings": {
    "error": "+5"
  },
  "js-post-build": {
    "cmd": "./node_modules/reason-react-native-monorepo/git-monorepo-usage-trick"
  }
}
